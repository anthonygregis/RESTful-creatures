<h2>Edit Creature Data</h2>
<form method="POST" action="/dinosaurs/<%= dinosaurID%>?_method=put">
    <label for="dinosaurName">Name</label>
    <input type="text" id="dinosaurName" name="name" value="<%= myDinosaur.name %>">

    <label for="dinosaurType">Type</label>
    <input type="text" id="dinosaurType" name="type" value="<%= myDinosaur.type %>">

    <input type="submit">
</form>